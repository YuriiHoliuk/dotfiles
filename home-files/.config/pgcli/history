+WITH missing_xp_data AS (
+          SELECT DISTINCT
+            tr.user_id,
+            tr.test_task_id,
+            tr.test_task_attempt_id,
+            tr.created_at as review_created_at,
+            tt.estimated_time_to_solve,
+            CEIL(tt.estimated_time_to_solve::numeric / ${MINUTES_PER_XP}) as xp_to_grant,
+            lu.id as leaderboard_user_id,
+            'TASK_COMPLETED' as activity_type
+          FROM tasks_reviews tr
+          JOIN test_tasks tt ON tr.test_task_id = tt.id
+          JOIN test_task_attempts tta ON tr.test_task_attempt_id = tta.id
+          JOIN leaderboard_users lu ON tr.user_id = lu.user_id AND lu.deleted_at IS NULL
+          LEFT JOIN leaderboard_user_xp_history xph ON 
+            xph.user_id = tr.user_id 
+            AND xph.test_task_id = tr.test_task_id
+            AND xph.activity_type = 'TASK_COMPLETED'
+          WHERE tr.status = 'APPROVED_AI'
+            AND tt.flow = 'ATTACHMENT'
+            AND tr.deleted_at IS NULL
+            AND tt.deleted_at IS NULL
+            AND tta.deleted_at IS NULL
+            AND tta.status = 'PASSED'
+            AND tt.estimated_time_to_solve IS NOT NULL
+            AND tr.is_last = true
+            AND xph.id IS NULL
+            AND tr.created_at >= '2025-07-09'::timestamp
+        )
+        SELECT * FROM missing_xp_data
+        ORDER BY review_created_at;

