+WITH patch AS (
+  SELECT
+    tt.id AS test_task_id,
+    (
+      CASE WHEN ctt.config ? 'isAIReviewEnabled'
+            AND ctt.config->>'isAIReviewEnabled' = 'true'
+        THEN jsonb_build_object('isAIReviewEnabled', ctt.config->'isAIReviewEnabled')
+        ELSE '{}'::jsonb END
+      ||
+      CASE WHEN ctt.config ? 'notRequiresReview'
+            AND ctt.config->>'isAIReviewEnabled' = 'true'
+        THEN jsonb_build_object('notRequiresReview', ctt.config->'notRequiresReview')
+        ELSE '{}'::jsonb END
+      ||
+      CASE WHEN ctt.config ? 'isMentorReviewDisabled'
+            AND ctt.config->>'isAIReviewEnabled' = 'true'
+        THEN jsonb_build_object('isMentorReviewDisabled', ctt.config->'isMentorReviewDisabled')
+        ELSE '{}'::jsonb END
+    ) AS delta
+  FROM course_topic_tasks AS ctt
+  JOIN test_tasks AS tt ON tt.id = ctt.test_task_id
+  WHERE tt.flow = 'ATTACHMENT'
+    AND ctt.config->>'isAIReviewEnabled' = 'true'
+)
+UPDATE test_tasks AS tt
+SET config = tt.config || patch.delta
+FROM patch
+WHERE tt.id = patch.test_task_id
+  AND patch.delta <> '{}'::jsonb;

