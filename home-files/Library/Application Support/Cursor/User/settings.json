{
    "search.exclude": {
        "**/*.code-search": true,
        "**/*.generated.ts": true,
        "**/allure-results": true,
        "**/bower_components": true,
        "**/generated.ts": true,
        "**/node_modules": true,
        "frontend/.next": true,
        "frontend/allure-results": true,
        "frontend/build-cache": true
    },
    "editor.tabSize": 2,
    "workbench.colorCustomizations": {
        "activityBarBadge.background": "#616161",
        "list.activeSelectionForeground": "#616161",
        "list.inactiveSelectionForeground": "#616161",
        "list.highlightForeground": "#616161",
        "scrollbarSlider.activeBackground": "#61616150",
        "editorSuggestWidget.highlightForeground": "#616161",
        "textLink.foreground": "#616161",
        "progressBar.background": "#616161",
        "pickerGroup.foreground": "#616161",
        "tab.activeBorder": "#616161",
        "notificationLink.foreground": "#616161",
        "editorWidget.resizeBorder": "#616161",
        "editorWidget.border": "#616161",
        "settings.modifiedItemIndicator": "#616161",
        "settings.headerForeground": "#616161",
        "panelTitle.activeBorder": "#616161",
        "breadcrumb.activeSelectionForeground": "#616161",
        "menu.selectionForeground": "#616161",
        "menubar.selectionForeground": "#616161",
        "statusBar.background": "#005f5f",
        "statusBar.noFolderBackground": "#005f5f",
        "statusBar.debuggingBackground": "#005f5f",
        "statusBar.foreground": "#ffffff",
        "statusBar.debuggingForeground": "#ffffff"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "workbench.sideBar.location": "left",
    "breadcrumbs.enabled": true,
    "editor.fontFamily": "\"MesloLGS Nerd Font Mono\"",
    "editor.fontLigatures": true,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "comment",
                    "entity.name.type.class",
                    "keyword",
                    "constant",
                    "storage.modifier",
                    "storage.type.class.js"
                ],
                "settings": {
                    "fontStyle": "italic"
                }
            },
            {
                "scope": [
                    "invalid",
                    "keyword.operator",
                    "constant.numeric.css",
                    "keyword.other.unit.px.css",
                    "constant.numeric.decimal.js",
                    "constant.numeric.json"
                ],
                "settings": {
                    "fontStyle": ""
                }
            }
        ]
    },
    "files.eol": "\n",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "emmet.includeLanguages": {
        "javascript": "javascriptreact",
        "vue-html": "html"
    },
    "terminal.integrated.fontFamily": "monospace",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.tabCompletion": "on",
    "workbench.editor.highlightModifiedTabs": true,
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": false,
    "merge-conflict.autoNavigateNextConflict.enabled": true,
    "workbench.editor.enablePreview": true,
    "workbench.editor.closeEmptyGroups": true,
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "workbench.iconTheme": "material-icon-theme",
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "pgsql.connections": [
        {
            "host": "mateacademy-database-development.cqw7zu22pde8.eu-central-1.rds.amazonaws.com",
            "dbname": "mate_development",
            "user": "dev",
            "password": "",
            "emptyPasswordInput": false,
            "port": "5432",
            "profileName": "DB Development"
        }
    ],
    "typescript.updateImportsOnFileMove.enabled": "always",
    "javascript.preferences.quoteStyle": "single",
    "typescript.preferences.quoteStyle": "single",
    "editor.minimap.enabled": false,
    "html.format.enable": false,
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "vim.easymotion": true,
    "vim.enableNeovim": true,
    "vim.foldfix": true,
    "vim.highlightedyank.enable": true,
    "vim.hlsearch": true,
    "vim.smartRelativeLine": true,
    "vim.sneak": true,
    "redhat.telemetry.enabled": true,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb",
        "*.png": "editor.excalidraw"
    },
    "workbench.colorTheme": "Default Dark Modern",
    "editor.suggestSelection": "first",
    "editor.semanticTokenColorCustomizations": null,
    "editor.fontSize": 14,
    "explorer.compactFolders": false,
    "explorer.autoReveal": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "security.workspace.trust.untrustedFiles": "open",
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": false,
        "scminput": false,
        "yaml": true
    },
    "github.copilot.editor.enableAutoCompletions": true,
    "editor.unicodeHighlight.nonBasicASCII": false,
    "githubPullRequests.pullBranch": "never",
    "settingsSync.ignoredExtensions": [
        "ms-vscode.vscode-speech",
        "github.copilot",
        "github.copilot-chat"
    ],
    "files.autoSave": "afterDelay",
    "editor.accessibilitySupport": "off",
    "javascript.preferences.importModuleSpecifier": "non-relative",
    "vim.statusBarColorControl": true,
    "vim.statusBarColors.visualline": "",
    "git.autofetch": true,
    "codemetrics.basics.EnabledForHTML": false,
    "codemetrics.nodeconfiguration.JsxElement": 0,
    "codemetrics.nodeconfiguration.JsxSelfClosingElement": 0,
    "codemetrics.nodeconfiguration.LabeledStatement": 0,
    "diffEditor.useInlineViewWhenSpaceIsLimited": false,
    "typescript.preferences.importModuleSpecifier": "non-relative",
    "cursor.cpp.disabledLanguages": [
        "plaintext"
    ],
    "multiCommand.commands": [
        {
            "command": "multiCommand.formatAndSave",
            "sequence": [
                "editor.action.organizeImports",
                "workbench.action.files.save",
                "workbench.action.files.save"
            ]
        }
    ],
    "typescript.preferences.preferTypeOnlyAutoImports": true,
    "typescript.updateImportsOnPaste.enabled": true,
    "javascript.updateImportsOnPaste.enabled": true,
    "roo-cline.allowedCommands": [
        "npm test",
        "npm install",
        "tsc",
        "git log",
        "git diff",
        "git show",
        "git commit",
        "git status",
        "gh",
        "npm run",
        "make",
        "fresh",
        "git",
        "ls",
        "cd",
        "pwd"
    ],
    "cSpell.userWords": [
        "uuidv"
    ]
}