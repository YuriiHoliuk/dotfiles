+INSERT INTO granted_permissions (course_id, permission, resources,
+   created_at, updated_at, created_by, updated_by, role_id, user_id,
+   english_level_id, subscription_grant_id, domain_id)
+  SELECT
+      course_id,
+      'study:AccessHiddenModule' as permission,
+      resources,
+      NOW() as created_at,
+      NOW() as updated_at,
+      created_by,
+      updated_by,
+      role_id,
+      user_id,
+      english_level_id,
+      subscription_grant_id,
+      domain_id
+  FROM granted_permissions
+  WHERE permission = 'HIDDEN_MODULE::VIEW'
+  AND NOT EXISTS (
+      SELECT 1 FROM granted_permissions gp2
+      WHERE gp2.permission = 'study:AccessHiddenModule'
+      AND COALESCE(gp2.course_id, -1) =
+  COALESCE(granted_permissions.course_id, -1)
+      AND COALESCE(gp2.role_id, -1) =
+  COALESCE(granted_permissions.role_id, -1)
+      AND COALESCE(gp2.user_id, -1) =
+  COALESCE(granted_permissions.user_id, -1)
+      AND COALESCE(gp2.english_level_id, -1) =
+  COALESCE(granted_permissions.english_level_id, -1)
+      AND COALESCE(gp2.subscription_grant_id, -1) =
+  COALESCE(granted_permissions.subscription_grant_id, -1)
+      AND COALESCE(gp2.domain_id, -1) =
+  COALESCE(granted_permissions.domain_id, -1)
+      AND gp2.resources = granted_permissions.resources
+  );

