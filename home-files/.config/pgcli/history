+WITH ordered_topics AS (
+  SELECT id, ROW_NUMBER() OVER (ORDER BY COALESCE("order", 0), id) - 1 as new_order
+  FROM course_topics
+  WHERE course_id = 412 AND deleted_at IS NULL
+)
