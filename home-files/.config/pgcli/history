+WITH stuck_attempts AS (
+       SELECT DISTINCT
+         tta.id as attempt_id,
+         tta.status as current_status,
+         tta.test_task_id,
+         tta.user_id,
+         MAX(tr.created_at) FILTER (WHERE tr.status = 'APPROVED') as approved_at,
+         COUNT(*) FILTER (WHERE tr.status = 'EXPIRE_AI') as expire_ai_count
+       FROM test_task_attempts tta
+       JOIN tasks_reviews tr ON tr.test_task_attempt_id = tta.id
+       WHERE tta.status = 'AI_REVIEW'
+       GROUP BY tta.id, tta.status, tta.test_task_id, tta.user_id
+       HAVING COUNT(*) FILTER (WHERE tr.status = 'APPROVED') > 0
+     )
+     SELECT
+       sa.attempt_id,
+       sa.current_status,
+       sa.approved_at,
+       sa.expire_ai_count,
+       tt.slug as task_slug,
+       u.email as user_email
+     FROM stuck_attempts sa
+     JOIN test_tasks tt ON tt.id = sa.test_task_id
+     JOIN users u ON u.id = sa.user_id
+     ORDER BY sa.approved_at DESC;

