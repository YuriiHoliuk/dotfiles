+SELECT
+    tt.name "testTaskName",
+    tt.id "testTaskId",
+    tr.test_task_attempt_id "testTaskAttemptId",
+    tr.user_id "userId",
+    tt.estimated_time_to_solve "estimatedTimeToSolve",
+    tr.created_at "createdAt",
+    lu.id "leaderboardUserId"
+FROM course_topic_tasks ctt
+JOIN test_tasks tt ON tt.id = ctt.test_task_id
+JOIN tasks_reviews tr ON tr.test_task_id = ctt.test_task_id
+INNER JOIN leaderboard_users lu ON lu.user_id = tr.user_id
+LEFT JOIN leaderboard_user_xp_history xph ON xph.leaderboard_user_id = lu.id
+    AND xph.test_task_id = tt.id
+WHERE ctt.id IN (
+    -- employment-ua
+    7160, 7302, 7303,
+    -- gen-ai-essentials-ua
+    8908, 8911, 8909, 8912, 8913, 8914, 8915, 8917, 8918, 8920, 8921, 8923, 8925,
+    -- da-sql-advanced-ua
