+WITH enabled_tasks AS (
+       SELECT unnest(ARRAY[
+           -- employment-ua
+           4288, 4287, 4201,
+           -- gen-ai-essentials-ua
+           4635, 4636, 4633, 4638, 4640, 4621, 4620, 4619, 4627, 4625, 4628, 4626, 4632,
+           -- da-sql-advanced-ua (excluding specific tasks)
+           4276, 4254, 4251, 4252, 4253, 4274, 4265, 4267, 4266, 4249, 4250, 4248, 4246, 4244, 4245, 4247,
+           4278, 4279, 4281, 4280, 4283, 4285, 4263, 4261, 4262, 4258, 4259, 4260, 4256, 4257, 4269, 4270,
+           4586, 4271, 4588, 4587,
+           -- da-product-ab-testing-ua
+           4407, 4399, 4401, 4398, 4402, 4382, 4383, 4377, 4381, 4380, 4374, 4375, 4372
+       ]) AS task_id
+   )
+   SELECT
+       tt.slug,
+       tt."name",
+       COUNT(CASE WHEN tr.status = 'REJECTED_AI' THEN 1 END) AS rejected_count,
+       COUNT(CASE WHEN tr.status = 'APPROVED' THEN 1 END) AS approved_count,
+       COUNT(*) AS total_reviews,
+       ROUND(
+           COUNT(CASE WHEN tr.status = 'APPROVED' THEN 1 END)::numeric /
+           NULLIF(COUNT(*), 0) * 100,
+           2
+       ) AS approval_ratio_percentage
+   FROM tasks_reviews tr
+   JOIN enabled_tasks et ON et.task_id = tr.test_task_id
+   JOIN test_tasks tt ON tt.id = tr.test_task_id
+   WHERE tr.status IN ('REJECTED_AI', 'APPROVED')
+   GROUP BY tt.slug, tt."name"
+   ORDER BY rejected_count DESC;

